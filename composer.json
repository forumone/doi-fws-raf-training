{
    "name": "fws/raf",
    "description": "Project starter for FWS RAF applications",
    "license": "GPL-2.0-or-later",
    "type": "project",
    "require": {
        "composer/installers": "^2.1",
        "cweagans/composer-patches": "^1.7",
        "drupal/acquia_search": "^3.1",
        "drupal/admin_toolbar": "^3.4",
        "drupal/admin_toolbar_tools": "^3.4",
        "drupal/allowed_formats": "^3.0",
        "drupal/better_exposed_filters": "^7.0",
        "drupal/bootstrap": "^3.32",
        "drupal/bpmn_io": "^2.0",
        "drupal/charts": "^5.1",
        "drupal/charts_highcharts_maps": "^1.0",
        "drupal/components": "^3.0@beta",
        "drupal/core-composer-scaffold": "^10.3",
        "drupal/core-recommended": "^10.3",
        "drupal/datatables": "^2.0",
        "drupal/date_filter": "^1.0",
        "drupal/default_content": "^2.0@alpha",
        "drupal/eca": "^2.0",
        "drupal/erd": "^2.0",
        "drupal/features": "^3.14",
        "drupal/field_group": "^3.4",
        "drupal/field_permissions": "^1.4",
        "drupal/linkit": "^6.1",
        "drupal/login_gov": "^1.1",
        "drupal/metatag": "^2.0",
        "drupal/node_revision_delete": "^2.0@RC",
        "drupal/paragraphs": "^1.17",
        "drupal/paragraphs_ee": "^10.0",
        "drupal/paragraphs_features": "^2.0",
        "drupal/pathauto": "^1.12",
        "drupal/quiz": "^6.0@alpha",
        "drupal/redirect": "^1.9",
        "drupal/rules": "^4.0",
        "drupal/social_media_links": "^2.10",
        "drupal/svg_image_responsive": "^3.0",
        "drupal/taxonomy_access_fix": "^4.0",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^3.3",
        "drupal/viewfield": "^3.0@beta",
        "drupal/views_year_filter": "^2.1",
        "drupal/visualization_charts": "^1.3",
        "drupal/we_megamenu": "^1.16",
        "drupal/webform": "^6.2",
        "drupal/webform_ui": "^6.2",
        "drupal/webp": "^1.0@RC",
        "drush/drush": "^11 || ^12 || ^13",
        "highcharts/maps": "11.1.0",
        "kenwheeler/slick": "^1.8.1",
        "oomphinc/composer-installers-extender": "^2.0",
        "owlcarousel2/owlcarousel2": "^2.3.4",
        "promet/fws-gov": "dev-promet",
        "select2/select2": "4.0.10",
        "vlucas/phpdotenv": "^5.1",
        "webflo/drupal-finder": "^1.2"
    },
    "require-dev": {
        "drupal/core-dev": "^10.2.0",
        "ergebnis/composer-normalize": "^2.42"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "promet/fws-gov",
                "version": "dev-promet",
                "source": {
                    "type": "git",
                    "url": "https://gitlab.djcase.com/promet/fws-gov.git",
                    "reference": "promet"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "kenwheeler/slick",
                "type": "drupal-library",
                "version": "1.8.1",
                "source": {
                    "type": "git",
                    "url": "https://github.com/kenwheeler/slick",
                    "reference": "v1.8.1"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "select2/select2",
                "type": "drupal-library",
                "version": "4.0.10",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/select2/select2/archive/develop.zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "owlcarousel2/owlcarousel2",
                "type": "drupal-library",
                "version": "2.3.4",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/OwlCarousel2/OwlCarousel2/archive/develop.zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "highcharts/maps",
                "type": "drupal-library",
                "version": "11.1.0",
                "require": {
                    "composer/installers": "^1.0 || ^2.0"
                },
                "extra": {
                    "installer-name": "highcharts_maps"
                },
                "dist": {
                    "type": "file",
                    "url": "https://code.highcharts.com/maps/11.1.0/modules/map.js"
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "ergebnis/composer-normalize": true,
            "oomphinc/composer-installers-extender": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": true
        },
        "discard-changes": true,
        "sort-packages": true
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "Suppress unexpected type error when Recipes are applied": "patches/recipes-unexpected-type.patch"
            },
            "drupal/we_megamenu": {
                "Issue #3282040: Attempt to modify property \"col_content\" on null": "https://www.drupal.org/files/issues/2024-04-10/3282040_7_0.patch"
            }
        }
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "cp -r vendor/promet/fws-gov/docroot/themes/custom/fws web/themes/custom/",
            "cp -r vendor/promet/fws-gov/docroot/themes/custom/fws_gov web/themes/custom/",
            "cp -r vendor/promet/fws-gov/docroot/modules/custom/fws_search web/modules/custom/"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "cp -r vendor/promet/fws-gov/docroot/themes/custom/fws web/themes/custom/",
            "cp -r vendor/promet/fws-gov/docroot/themes/custom/fws_gov web/themes/custom/",
            "cp -r vendor/promet/fws-gov/docroot/modules/custom/fws_search web/modules/custom/"
        ]
    }
}
