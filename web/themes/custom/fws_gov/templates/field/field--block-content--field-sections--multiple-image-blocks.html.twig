<div class="block-mulitiple-image-blocks">
{% for item in items %}
    {% if item.content['#paragraph'].field_link.0.url.external or item.content['#paragraph'].field_link.0.url.isRouted == false %}
        {% set linkurl = item.content['#paragraph'].field_link.0.uri %}
    {% else %}
        {% set linkurl = path(item.content['#paragraph'].field_link.0.url.routeName, item.content['#paragraph'].field_link.0.url.routeParameters,item.content['#paragraph'].field_link.0.url.options) %}
    {% endif %}
    <div {{ attributes.addClass('our-focus-card') }} }}>
        <a href="{{linkurl}}" title="{{item.content['#paragraph'].field_title.0.value}}">
            <div class="card-image">
                <img src="{{item.content['#paragraph'].field_image.0.entity.field_media_image.entity.uri.value  | image_style('medium_block')}}"
                     alt="{{item.content['#paragraph'].field_image.0.entity.field_media_image.0.alt}}" loading="lazy">
                <h3 class="card-title">{{item.content['#paragraph'].field_title.0.value}}</h3>
            </div>
        </a>
    </div>
{% endfor %}
</div>
