{# Menu Block Template - block--system-menu-block.html.twig #}
{% set heading_id = attributes.id ~ '-menu'|clean_id %}
<nav role="navigation" aria-labelledby="{{ heading_id }}" class="navbar navbar-main"{{ attributes|without('role', 'aria-labelledby', 'class') }}>
  <div class="navbar-header">
    {% if not configuration.label_display %}
      {% set title_attributes = title_attributes.addClass('visually-hidden') %}
    {% endif %}
    {{ title_prefix }}
    <h2{{ title_attributes.setAttribute('id', heading_id) }}>{{ configuration.label }}</h2>
    {{ title_suffix }}
  </div>

  <div class="hidden-xs">
    {{ content }}
  </div>
</nav>
